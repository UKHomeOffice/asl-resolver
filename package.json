    "@asl/schema": "^10.16.0",
    "@asl/service": "^9.1.5",
    "aws-sdk": "^2.814.0",
    "hot-shots": "^6.8.2",
    "jsondiffpatch": "^0.4.1",
    "jsonwebtoken": "^8.3.0",
